<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventListFragment"
    android:background="@color/blue_gray">

    <RelativeLayout
        android:id="@+id/eventRelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:layout_marginTop="5dp">

        <TextView
            android:id="@+id/eventID_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/event_id_number"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/eventID_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/eventID_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/dateTime_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/eventID_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/date_time"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/dateTime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/eventID_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/dateTime_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/latLong_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dateTime_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/lat_long"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/latLong_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/dateTime_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/latLong_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/heading_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/latLong_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/heading"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/heading_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/latLong_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/heading_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/satStatus_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/heading_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/satellite_status"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/satStatus_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/heading_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/satStatus_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/odometer_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/satStatus_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/odometer"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/odometer_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/satStatus_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/odometer_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/velocity_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/odometer_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/velocity"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/velocity_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/odometer_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/velocity_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/engineHours_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/velocity_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/engine_hours"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/engineHours_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/velocity_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/engineHours_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/rpm_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/engineHours_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/rpm"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/rpm_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/engineHours_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/rpm_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/driver_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rpm_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/driver"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/driver_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rpm_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/driver_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/vehicle_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/driver_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/vehicle"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/vehicle_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/driver_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/vehicle_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/trailer_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vehicle_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/trailer"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/trailer_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vehicle_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/trailer_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/eventCount_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/trailer_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/event_count"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/eventCount_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/trailer_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/eventCount_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/MacAddress_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/eventCount_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/mac_address"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/MacAddress_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/eventCount_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/MacAddress_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/trackerModel_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/MacAddress_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/tracker_model"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/trackerModel_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/MacAddress_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/trackerModel_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/serial_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/trackerModel_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/serial"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/serial_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/trackerModel_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/serial_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/version_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/serial_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/version"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/version_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/serial_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/version_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/vin_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/version_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/vin"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/vin_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/version_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/vin_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/sequence_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vin_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/sequence"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/sequence_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vin_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/sequence_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/speed_lbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sequence_lbl"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:text="@string/speed"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/speed_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sequence_tv"
            android:layout_marginVertical="5dp"
            android:layout_toEndOf="@+id/speed_lbl"
            android:text="@string/dash"
            android:textColor="@color/colorAlert"
            android:textSize="6pt"
            tools:ignore="TextContrastCheck" />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentBottom="true"
            android:gravity="center">

            <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
                android:id="@+id/getData_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/get_data"
                android:layout_marginVertical="5dp"
                android:gravity="center"
                android:contentDescription="@string/get_data"/>

            <ImageButton
                android:id="@+id/refreshData_btn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginHorizontal="10dp"
                android:contentDescription="@string/refresh_data"
                android:src="@drawable/ic_baseline_refresh_24"
                tools:ignore="ImageContrastCheck" />


        </androidx.appcompat.widget.LinearLayoutCompat>
    </RelativeLayout>

</FrameLayout>